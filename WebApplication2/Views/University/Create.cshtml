@model WebApplication2.Models.University
@{
    ViewBag.Title = "Добавление пользователи";
}
<form asp-action="Create"asp-controller="University">
    <div class="form-group">
        <label asp-for="Name" class="control-label">Название факультета</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Rating" class="control-label">Рейтинг</label>
         <input type="text" asp-for="Rating" class="form-control" />
     </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default"/>
    </div>
</form>
